config system
        option hostname 'LTEBOX'
        option conloglevel '8'
        option zonename 'Asia/Shanghai'
        option timezone 'CST-8'
        option cronloglevel '5'
        option zram_comp_algo 'lzo'

config timeserver 'ntp'
        option enabled '1'
        option enable_server '0'
        list server 'ntp1.aliyun.com'
        list server 'ntp2.aliyun.com'
        list server 'ntp3.aliyun.com'
        list server 'ntp4.aliyun.com'

config gpio_switch 'cpe_pwr'
        option name 'CPE Power'
        option gpio_pin '13'
        option value '1'

config gpio_switch 'usb_pwr'
        option name 'CPE Power'
        option gpio_pin '14'
        option value '1'

config gpio_switch 'sim_sel'
        option name 'CPE Power'
        option gpio_pin '39'
        option value '0'

config led 'led_lan'
        option name 'lan'
        option sysfs 'blue:link2'
        option trigger 'netdev'
        option mode 'rx'
        option dev 'eth0.1'

config led 'led_wan'
        option name 'wan'
        option sysfs 'blue:link1'
        option trigger 'netdev'
        option mode 'rx'
        option dev 'eth0.2'

config led 'led_wifi'
        option name 'wifi'
        option sysfs 'blue:wifi'
        option trigger 'netdev'
        option mode 'link'
        option dev 'wlan0'