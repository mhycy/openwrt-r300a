config wifi-device 'radio1'
        option type 'mac80211'
        option path 'platform/10180000.wmac'
        option channel 'auto'
        option band '2g'
        option htmode 'HT20'
        option cell_density '0'
        option txpower '20'
        option country 'CN'

config wifi-iface 'wlan0'
        option device 'radio1'
        option network 'lan'
        option mode 'ap'
        option ssid 'EH-LTE'
        option encryption 'psk2'
        option key '82227356'